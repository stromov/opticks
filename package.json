{
  "name": "opticks",
  "version": "4.0.0",
  "description": "FindHotel Toggle Flag JavaScript SDK",
  "main": "lib/simple.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean:booleanToggle": "jscodeshift --transform lib/transform/booleanToggle.js",
    "clean:toggle": "jscodeshift --transform lib/transform/toggle.js",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "jscodeshift": "jscodeshift",
    "prettier-eslint": "prettier-eslint",
    "prepare": "npm run prepare:flow && npm run build",
    "prepare:flow": "flow-copy-source src lib -i test.js",
    "test": "jest src",
    "test:watch": "jest --watch src"
  },
  "keywords": [
    "experiment",
    "ab testing",
    "feature-flag",
    "toggle",
    "flag",
    "canary",
    "codemod",
    "jscodeshift"
  ],
  "author": "Jop de Klein",
  "license": "ISC",
  "peerDependencies": {
    "@optimizely/optimizely-sdk": "4.2.1",
    "jscodeshift": "^0.14.0"
  },
  "devDependencies": {
    "@optimizely/optimizely-sdk": "4.2.1",
    "@types/jest": "^29.4.0",
    "jest": "^23.6.0",
    "jscodeshift": "^0.14.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
